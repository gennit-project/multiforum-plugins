  "description": "A simple demonstration plugin that logs when content is created or updated",
  "entry": "dist/index.js",
  "events": [
    "downloadableFile.created",
    "downloadableFile.updated",
    "downloadableFile.downloaded"
  ],
  "secrets": [],
  "metadata": {
    "author": {
      "name": "Multiforum Team",
      "url": "https://github.com/listical/multiforum-plugins"
    },
    "homepage": "https://github.com/listical/multiforum-plugins/tree/main/plugins/hello-world",
    "license": "MIT",
    "tags": [
      "example",
      "demo"
    ]
  },
  "documentation": {
    "readmePath": "README.md"
  },
  "ui": {
    "forms": {
      "server": [
        {
          "title": "About This Plugin",
